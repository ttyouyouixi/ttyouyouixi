<view class="page">
  <view class="weui-cells__title">根据出发日期筛选</view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_input">
     <view class="weui-cell__bd cell-title">出发日期</view>
      <view class="weui-cell__ft">
        <picker mode="date" value="{{fromDate}}" start="{{startDate}}" bindchange="bindDateChange">
          <view class="weui-input">{{fromDate}}</view>
        </picker>
      </view>
    </view>
  </view>
